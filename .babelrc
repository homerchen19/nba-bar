{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "import",
      {
        "libraryName": "antd-mobile"
      }
    ],
    "ramda",
    [
      "module-resolver",
      {
        "alias": {
          "@containers": "./renderer/containers",
          "@components": "./renderer/components",
          "@utils": "./renderer/utils"
        }
      }
    ]
  ]
}
